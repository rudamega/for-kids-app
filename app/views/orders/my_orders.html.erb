
<h1 class="text-center">HISTORICO DE ORDERS</h1>
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Order number</th>
      <th>QTY</th>
      <th>Item</th>
      <th>Price</th>
      <th>Date</th>
      <th>Bought from</th>
    </tr>
  </thead>
  <tbody>
 <%@order.each do |order| %>
      <tr>
        <td><%= order.id %></td>
        <td><%= 1 %></td>
        <td><%= order.item.name%></td>
        <td><%= order.item.price%></td>
        <td><%= order.created_at%></td>
        <td><%= link_to "Ver loja do #{order.item.user.first_name}", user_user_items_path(order.item.user.id)%></td>
      </tr>
  <%end%>
  </tbody>
</table>
