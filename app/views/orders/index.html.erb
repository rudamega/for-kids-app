
<h1>HISTORICO DE ORDERS</h1>

<div class="cards">
 
  
<table class="table">
  <thead>
    <tr>
      <th>Order number</th>
      <th>QTY</th>
      <th>Item</th>
      <th>Price</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
 <%@orders.each do |order| %>
      <tr>
        <td><%= order.id %></td>
        <td><%= 1 %></td>
        <td><%= order.item.name%></td>
        <td><%= order.item.price%></td>
        <td><%= order.created_at%></td>
      </tr>
  <%end%> 
  </tbody>
</table>
